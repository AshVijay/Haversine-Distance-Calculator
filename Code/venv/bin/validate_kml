#!/home/ashwin/PycharmProjects/Skylark_Assignment/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pykml==0.1.3','console_scripts','validate_kml'
__requires__ = 'pykml==0.1.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pykml==0.1.3', 'console_scripts', 'validate_kml')()
    )
